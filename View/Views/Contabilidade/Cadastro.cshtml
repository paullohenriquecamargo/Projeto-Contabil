
@{
    ViewBag.Title = "Cadastro";
}

<h2>Cadastro de Contabilidade</h2>

<form action="@Url.Action("Store")" method="post">
    <div class="row">
        <div class="col-12">
            <div class="form-group">
                <label for="campo-contabilidade">Contabilidade </label>
                <input type="text" name="contabilidade" id="campo-contabilidade" />                             
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-12">
            <button class="btn btn-success" type="submit"><i class="glyphicon-fast-backward fa-save"></i> Salvar</button>
        </div>
    </div>